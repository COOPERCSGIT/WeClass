@import '../../../weui.wxss';

.container {
  padding: 4px 12px;
}
.coursee{
    border: 2px solid #ffffff;
    border-radius: 5px;
}

.hint {
    display: block;
    margin: 8px 0;
    color: #999;
    font-size: 20px;
}

.course {
    height : 100px;
    width : 100%;
    border-top: 2px solid;
    border-left: 2px solid;
    border-bottom: 5px solid;
    border-right: 5px solid;
    border-radius: 8px;
    margin: 8px;
    border-color: lightseagreen;
}

.courseName{
    position: absolute;
    margin-top: 30px;
    margin-left: 50px; 
    font-size: 10mm;
}

.icon{
position: absolute;
margin-top: 30px;
margin-left: 300px;
}

.image {
    width: 100%;
    height: 200px;
    background-color: #eeeeee;
}

.title {
    padding: 14px;
    font-size: 14px;
}

